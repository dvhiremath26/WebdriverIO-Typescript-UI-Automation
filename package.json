  },
  "homepage": "https://github.com/dvhiremath26/WebdriverIO-Typescript-UI-Automation#readme",
  "devDependencies": {
    "@types/jasmine": "^6.0.0",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@wdio/allure-reporter": "^9.24.0",
    "@wdio/cli": "^9.24.0",
    "@wdio/globals": "^9.23.0",
