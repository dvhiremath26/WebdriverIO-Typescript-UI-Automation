    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "typescript-eslint": "^8.55.0",
    "wdio-wait-for": "^3.1.1"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run format"
    ],
    "*.yaml": [
      "npm run format"
